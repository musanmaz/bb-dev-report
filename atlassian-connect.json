{
  "key": "bb-dev-report",
  "name": "Bitbucket Developer Report",
  "baseUrl": "https://bb-dev-report.onrender.com",
  "apiVersion": 1,
  "authentication": {
    "type": "jwt"
  },
  "scopes": [
    "account", 
    "repository", 
    "pullrequest", 
    "webhook"
  ],
  "modules": {
    "repoPages": [
      {
        "url": "/report?repo={repository.full_name}",
        "key": "dev-report",
        "name": {
          "value": "Developer Report"
        }
      }
    ]
  }
}
